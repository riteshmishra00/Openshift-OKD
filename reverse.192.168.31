$TTL    604800
@       IN      SOA     dns.example.com. admin.example.com. (
                  6     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800     ; Negative Cache TTL
)

; name servers - NS records
    IN      NS      dns.example.com.

; name servers - PTR records
71    IN    PTR    dns.example.com.

; OpenShift Container Platform Cluster - PTR records
72    IN    PTR    bootstrap.ocp.example.com.
73    IN    PTR    master1.ocp.example.com.
74    IN    PTR    master2.ocp.example.com.
75    IN    PTR    master3.ocp.example.com.
76    IN    PTR    worker1.ocp.example.com.
77    IN    PTR    worker2.ocp.example.com.
71    IN    PTR    api.ocp.example.com.
71    IN    PTR    api-int.ocp.example.com.
